<wargamepatch>

	<!-- Remove US unspec decks-->
	
	<ndfpatch ndf="pc\ndf\patchable\gfx\everything.ndfbin" table="TDeckConstraints" name="US_unspec_removal">
		<matchconditions>
			<matchcondition type="property" property="Country">US</matchcondition>
			<matchcondition type="property" property="UnitType">0C565AD652360E00</matchcondition>
		</matchconditions>
		<changes>
			<change property="UnitType" type="LocalisationHash" operation="set"></change>
