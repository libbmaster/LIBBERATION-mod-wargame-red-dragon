<!-- create M60A3 CMD KE-->
	<ndfcreate ndf="pc\ndf\patchable\gfx\everything.ndfbin" table="TMountedWeaponDescriptor" name="M60A3_CMD_KE" />
<ndfpatch ndf="pc\ndf\patchable\gfx\everything.ndfbin" table="TTurretTwoAxisDescriptor" name="M60A3_CMD_KE">
		<matchconditions>
			<matchcondition type="referencedby" table="TWeaponManagerModuleDescriptor">
				<matchconditions>
					<matchcondition type="property" property="_ShortDatabaseName">WeaponDescriptor_Unit_CMD_M60A1</matchcondition>
				</matchconditions>
			</matchcondition>
			<matchcondition type="property" property="Tag">tourelle1</matchcondition>
		</matchconditions>
		<changes>
			<change operation="append" property="MountedWeaponDescriptorList" type="ObjectReference">
				<reference table="TMountedWeaponDescriptor">
					<matchconditions>	
						<matchcondition type="property" property="__order">last</matchcondition>
					</matchconditions>
				</reference>
			</change>
		</changes>
</ndfpatch>

